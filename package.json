{
  "name": "gatsby-remark-codesandbox",
  "description": "Adds syntax highlighting to code blocks at build time using PrismJS and CodeSandbox",
  "version": "1.2.9",
  "author": "Ives van Hoorne <ives.v.h@gmail.com>",
  "dependencies": {
    "babel-runtime": "^6.26.0",
    "git-branch": "^1.0.0",
    "git-username": "^0.5.0",
    "unist-util-visit": "^1.1.1"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.6.0",
    "cross-env": "^5.0.5",
    "remark": "^7.0.1"
  },
  "keywords": [
    "gatsby",
    "gatsby-plugin",
    "prismjs",
    "remark"
  ],
  "license": "MIT",
  "main": "index.js",
  "scripts": {
    "build": "babel src --out-dir . --ignore __tests__",
    "watch": "babel -w src --out-dir . --ignore __tests__",
    "prepublish": "cross-env NODE_ENV=production npm run build"
  }
}
